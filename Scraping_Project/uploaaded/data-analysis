    window.TOUCHSCREEN = false;
   window.addEventListener("touchstart",  '=&gt'); {
     document.body.classList.add("touch");
     window.TOUCHSCREEN = true;
   };
   </script>
   <script deferred="" src="https://www.classcentral.com/report/wp-content/themes/wp-theme/dist/webpack/cc-main.8ec3ba12afa3aada2eb4.js">
   </script>
   <script id="ppress-frontend-script-js-extra" type="text/javascript">
    /* &lt;![CDATA[ */
 var pp_ajax_form = {"ajaxurl":"https:\/\/www.classcentral.com\/report\/wp-admin\/admin-ajax.php","confirm_delete":"Are you sure?","deleting_text":"Deleting...","deleting_error":"An error occurred. Please try again.","nonce":"1e4861bb76","disable_ajax_form":"false"};
