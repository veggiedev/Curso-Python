    <section class="width-page padding-vert-xxlarge small-down-padding-horz-medium">
     <header>
      <h1 class="head-1 margin-bottom-medium">
       संस्था
      </h1>
      <ul class="medium-down-hidden row nowrap vert-align-top horz-align-center list-no-style margin-bottom-large">
       <li class="scale-on-hover width-1-3 xxlarge-up-width-1-4 padding-horz-xsmall border-box relative">
        <a class="hover-no-underline radius-small shadow-light bg-white padding-horz-medium padding-vert-large col height-100" data-track-ga='{"category":"catalog index featured","action":"Google","label":"institutions"}' href="/institution/google" title="Google online courses and reviews">
         <div class="margin-bottom-xlarge text-center">
          <img alt="Google logo" class="inline-block" src="https://ccweb.imgix.net/https%3A%2F%2Fwww.classcentral.com%2Fimages%2Flogos%2Finstitutions%2Fgoogle-hz.png?auto=format&amp;h=60&amp;ixlib=php-3.3.1&amp;s=a84b8555635079b60b8a705749ddc1cd" style="max-height: 40px; object-fit: contain;" width="85%"/>
         </div>
         <div class="text-right margin-bottom-xxsmall" style="margin-top: auto;">
          <span class="text-3 inline-block weight-bold color-charcoal bg-gray-light radius padding-horz-xsmall">
           1029
           <span class="large-down-hidden">
            पाठ्यक्रम
           </span>
          </span>
         </div>
        </a>
        <div class="absolute bottom left margin-left-large margin-bottom-large">
         <div class="cmpt-follow" data-button-follow='{"name":"Google","followers":"5,985"}' data-template="follow-institution-343-feature">
